{
    "name": "website",
    "version": "0.1.0",
    "description": "Website project for vue-es.org on Jekyll and Vue",
    "scripts": {
        "start": "concurrently \"npm run serve\" \"npm run dev\" --names \"SERVER,PACK\" --prefix name",
        "postinstall": "bundler install",
        "serve": "bundle exec jekyll serve --incremental",
        "dev": "webpack --env.NODE_ENV=development --watch",
        "prod": "webpack -p --env.NODE_ENV=production"
    },
    "author": "VUE-ES.ORG TEAM",
    "dependencies": {
        "primer-css": "^9.3.0",
        "vue": "^2.4.4",
        "vue-disqus": "^2.0.3"
    },
    "devDependencies": {
        "babel-cli": "^6.23.0",
        "babel-core": "^6.23.1",
        "babel-loader": "^6.4.0",
        "babel-preset-es2015": "^6.22.0",
        "browser-sync": "^2.18.8",
        "css-loader": "^0.28.7",
        "extract-text-webpack-plugin": "^2.1.0",
        "node-sass": "^4.5.3",
        "sass-loader": "^6.0.6",
        "vue-loader": "^11.1.4",
        "vue-style-loader": "^3.0.3",
        "vue-template-compiler": "^2.2.2",
        "webpack": "^2.2.1"
    }
}
